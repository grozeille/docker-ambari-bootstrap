                "ams_hbase_security_log_maxfilesize": "256",
                "content": "ams-hbase-log4j.properties"
            },
            "properties_attributes": {}
        },
        "ams-hbase-policy": {
            "properties": {
                "security.admin.protocol.acl": "*",
                "security.client.protocol.acl": "*",
                "security.masterregion.protocol.acl": "*"
